{
  "name": "radnt-cli",
  "version": "1.0.8",
  "description": "A powerful CLI tool for creating Next.js projects with shadcn/ui and modern development tools",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "bin": {
    "radnt": "dist/cli.js",
    "create-radnt-app": "dist/cli.js"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "test": "echo \"Tests will be added in future versions\" && exit 0",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "release": "node scripts/publish.js"
  },
  "keywords": [
    "nextjs",
    "shadcn",
    "cli",
    "react",
    "typescript",
    "tailwind",
    "development",
    "scaffold"
  ],
  "author": "Radnt Team",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "execa": "^5.1.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "semver": "^7.5.4",
    "socket.io": "^4.7.4",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/semver": "^7.5.4",
    "@types/validate-npm-package-name": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tubbymctubbzz/radnt-cli.git"
  },
  "bugs": {
    "url": "https://github.com/tubbymctubbzz/radnt-cli/issues"
  },
  "homepage": "https://github.com/tubbymctubbzz/radnt-cli#readme"
}
